{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["Android >= 2.3", "iOS >= 4"]
      }
    }],
    "es2015",
    "stage-2"
  ],
  "plugins": [["component", [
    {
      "libraryName": "qfpay-ui",
      "style": true
    }
  ]], "transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "es2015", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
